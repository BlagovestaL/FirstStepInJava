package first.steps.in.coding.exe;

import java.util.Scanner;

public class p05SuppliesForSchool {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        int him = Integer.parseInt(scanner.nextLine()); //himikalki
        int mark = Integer.parseInt(scanner.nextLine()); //markeri
        int prep = Integer.parseInt(scanner.nextLine()); // preparat
        double ots = Double.parseDouble(scanner.nextLine());


        double hi = him * 5.80;
        double ma = mark * 7.20;
        double pr = prep * 1.20;
        double x = hi + ma + pr;
        double ot = ots/100;


        System.out.println(x - ( x * ot));

    }
}
